{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "check",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo check"
		},
		{
			"type": "cargo",
			"command": "run",
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: cargo run",
			"group": "build"
		},
		{
			"type": "cargo",
			"command": "clean",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build"
			},
			"label": "rust: cargo clean"
		}
	]
}